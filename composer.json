{
    "name": "3x1io/filament-sitemap",
    "description": "Site Settings Like title, description, profile and Sitemap Generator",
    "keywords": [
        "laravel",
        "generator",
        "sitemap",
        "seo",
        "settings",
        "filament"
    ],
    "homepage": "https://3x1.io",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "io3x1\\FilamentSitemap\\": "src/"
        }
    },
    "authors": [
        {
            "name": "3x1",
            "email": "info@3x1.io",
            "role": "CEO"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "filament/filament": "^2.16",
        "filament/notifications": "^2.16",
        "filament/spatie-laravel-settings-plugin": "^2.8",
        "spatie/laravel-package-tools": "^1.10",
        "3x1io/filament-themes": "^1.1",
        "spatie/laravel-sitemap": "^6.2"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "io3x1\\FilamentSitemap\\FilamentSitemapProvider"
            ]
        }
    },
    "prefer-stable": true
}
